{
  "name": "binary-buffer-parser",
  "license": "MIT",
  "description": "A simple buffer parser.",
  "version": "0.9.3",
  "homepage": "https://github.com/Pupix/binary-buffer-parser",
  "author": {
    "name": "Robert Manolea",
    "email": "manolea.robert@gmail.com"
  },
  "contributors": [
    {
      "name": "Robert Manolea",
      "email": "manolea.robert@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Pupix/binary-buffer-parser.git"
  },
  "bugs": {
    "url": "https://github.com/Pupix/binary-buffer-parser/issues"
  },
  "dependencies": {
    "expandjs": "^0.9.0"
  },
  "devDependencies": {
    "browserify": "*",
    "minifyify": "*"
  },
  "scripts": {
    "build": "npm run build:browserify && npm run build:minify",
    "build:browserify": "browserify index -i expandjs --standalone BufferParser > dist/binary-buffer-parser.js",
    "build:minify": "browserify -d index -i expandjs --standalone BufferParser -p [minifyify --map dist/binary-buffer-parser.map.json --output dist/binary-buffer-parser.map.json] > dist/binary-buffer-parser.min.js"
  },
  "keywords": [
    "binary",
    "parser"
  ]
}
